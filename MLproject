name: pytorch_test

conda_env: conda.yaml

entry_points:
  main:
    parameters:
      learning_rate: {type: float, default: 0.0001}
      batch_size: {type: int, default: 8}
      num_epochs: {type: int, default: 1}
    command: "python train.py --lr {learning_rate} --batch_size {batch_size} --num_epochs {num_epochs}"